INSERT INTO advice (name, content, video_url, created_at)
VALUES 
('1 кеңес', 'Күніне 10 минут тыныс алу жаттығуын орындаңыз.', 'https://youtu.be/example1', '2025-11-09 19:00:00'),
('2 кеңес', 'Таңертең күн сәулесіне шығып, серуендеңіз.', 'https://youtu.be/example2', '2025-11-09 19:10:00'),
('3 кеңес', 'Ұйықтар алдында 5 минут тыныштықта отырыңыз — миыңыз тынығады.', 'https://youtu.be/example3', '2025-11-09 20:00:00'),
('4 кеңес', 'Күніне кемінде 1,5 литр су ішуді ұмытпаңыз.', 'https://youtu.be/example4', '2025-11-09 20:10:00'),
('5 кеңес', 'Таңертең бір стақан жылы су ішіңіз — ағзаны оятуға көмектеседі.', 'https://youtu.be/example5', '2025-11-09 20:20:00'),
('6 кеңес', 'Күні бойы әр сағат сайын аздап қозғалыс жасап тұрыңыз.', 'https://youtu.be/example6', '2025-11-09 20:30:00'),
('7 кеңес', 'Кешке дейін телефонды жиі қолданбауға тырысыңыз, көзіңізге дем беріңіз.', 'https://youtu.be/example7', '2025-11-09 20:40:00'),
('8 кеңес', 'Аптасына бір рет табиғатта серуендеңіз, бұл күйзелісті азайтады.', 'https://youtu.be/example8', '2025-11-09 20:50:00');

-- ===== Жаттығулар (exercise) =====
INSERT INTO exercise (name, description, video_url, created_at)
VALUES 
('1 жаттығу', 'Терең тыныс алу: Мұрынмен 5 секунд тыныс алып, 5 секунд ұстап, ауызбен шығарыңыз.', 'https://youtu.be/exercise1', '2025-11-09 19:15:00'),
('2 жаттығу', 'Күн сайын 15 минут йога немесе жеңіл созылу жаттығуларын орындаңыз.', 'https://youtu.be/exercise2', '2025-11-09 19:20:00'),
('3 жаттығу', 'Иық айналдыру: 10 рет алға, 10 рет артқа — қан айналымды жақсартады.', 'https://youtu.be/exercise3', '2025-11-09 21:00:00'),
('4 жаттығу', 'Қол бұлғау: 30 секунд бойы қолды кең сермеп, денені бос ұстаңыз.', 'https://youtu.be/exercise4', '2025-11-09 21:10:00'),
('5 жаттығу', 'Көз жаттығуы: алыстағы нысанға 5 секунд қараңыз, сосын жақындағыға ауысыңыз.', 'https://youtu.be/exercise5', '2025-11-09 21:20:00'),
('6 жаттығу', 'Мойын айналдыру: басты жаймен оңға және солға айналдырыңыз.', 'https://youtu.be/exercise6', '2025-11-09 21:30:00');

